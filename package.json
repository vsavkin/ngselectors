{
  "name": "ngselectors",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/index.js",
  "jsnext:main": "./dist/esm/index.js",
  "scripts": {
    "karma": "karma start",
    "test": "npm run build; karma start",
    "build_watch": "rm -rf dist; tsc -w",
    "build": "rm -rf dist; tsc",
    "build_npm": "rm -rf dist && tsc -p tsconfig.publish.es5.json && tsc -p tsconfig.publish.esm.json",
    "postbuild_npm": "cp package.json README.md dist/ && npm run rewrite_npm_package",
    "devsetup": "typings install",
    "rewrite_npm_package": "node --harmony_destructuring tools/rewrite-published-package.js",
    "build_bundle": "cp -r src ngselectors && tsc typings/index.d.ts src/ngselectors.ts --rootDir . --module system -t es5 --outFile dist/bundles/ngselectors.js  --moduleResolution node --emitDecoratorMetadata --experimentalDecorators",
    "lint": "tslint \"src/**/*.ts\"",
    "format": "clang-format -style=file -i --glob=src/**/*.ts"
  },
  "keywords": [
    "angular2",
    "angular",
    "redux"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vsavkin/ngselectors.git"
  },
  "author": "vsavkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vsavkin/ngselectors/issues"
  },
  "homepage": "https://github.com/vsavkin/ngselectors#readme",
  "dependencies": {
    "@angular/common": "https://github.com/angular/common-builds",
    "@angular/compiler": "https://github.com/angular/compiler-builds",
    "@angular/core": "https://github.com/angular/core-builds",
    "@angular/platform-browser": "https://github.com/angular/platform-browser-builds",
    "@angular/platform-browser-dynamic": "https://github.com/angular/platform-browser-dynamic-builds",
    "rxjs": "5.0.0-beta.6"
  },
  "devDependencies": {
    "es6-module-loader": "^0.17.10",
    "es6-shim": "^0.35.0",
    "http-server": "^0.8.5",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "json": "^9.0.3",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.6",
    "karma-systemjs": "^0.10.0",
    "karma-sourcemap-loader": "~0.3",
    "parse5": "^1.3.2",
    "reflect-metadata": "0.1.2",
    "systemjs": "^0.19.16",
    "systemjs-builder": "^0.15.7",
    "typescript": "^2.0.0",
    "zone.js": "^0.6.6",
    "clang-format": "^1.0.35",
    "tslint": "^3.6.0",
    "@types/jasmine": "^2.2.33",
    "codelyzer": "0.0.14"
  },
  "typings": "dist/index.d.ts"
}
